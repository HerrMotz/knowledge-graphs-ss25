<?xml version="1.0"?>
<rdf:RDF xmlns="http://becks.org/ontology#"
     xml:base="http://becks.org/ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:becks="http://becks.org/ontology#">
    <owl:Ontology rdf:about="http://becks.org/ontology#"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2002/07/owl#qualifiedCardinality -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#qualifiedCardinality"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://becks.org/ontology#enthaeltZutat -->

    <owl:ObjectProperty rdf:about="http://becks.org/ontology#enthaeltZutat">
        <rdfs:domain rdf:resource="http://becks.org/ontology#Pizza"/>
        <rdfs:range rdf:resource="http://becks.org/ontology#Zutat"/>
    </owl:ObjectProperty>
    


    <!-- http://becks.org/ontology#gehörtZuPizzeria -->

    <owl:ObjectProperty rdf:about="http://becks.org/ontology#gehörtZuPizzeria">
        <rdfs:domain rdf:resource="http://becks.org/ontology#Pizza"/>
        <rdfs:range rdf:resource="http://becks.org/ontology#Pizzeria"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://becks.org/ontology#adresse -->

    <owl:DatatypeProperty rdf:about="http://becks.org/ontology#adresse">
        <rdfs:domain rdf:resource="http://becks.org/ontology#Pizzeria"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://becks.org/ontology#bewertungAufGoogle -->

    <owl:DatatypeProperty rdf:about="http://becks.org/ontology#bewertungAufGoogle">
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1.0</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">5.0</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://becks.org/ontology#groesse -->

    <owl:DatatypeProperty rdf:about="http://becks.org/ontology#groesse">
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0</xsd:minExclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://becks.org/ontology#preis -->

    <owl:DatatypeProperty rdf:about="http://becks.org/ontology#preis">
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0</xsd:minInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://becks.org/ontology#Glutenfreie -->

    <owl:Class rdf:about="http://becks.org/ontology#Glutenfreie">
        <rdfs:subClassOf rdf:resource="http://becks.org/ontology#Zutat"/>
        <rdfs:comment>Enthält alle möglichen glutenfreien Zutaten einer Pizza als Instanzen, damit inferiert werden kann, ob eine Pizza glutenfrei ist.</rdfs:comment>
        <rdfs:label>Glutenfreie Zutaten</rdfs:label>
    </owl:Class>
    


    <!-- http://becks.org/ontology#Käse -->

    <owl:Class rdf:about="http://becks.org/ontology#Käse">
        <rdfs:subClassOf rdf:resource="http://becks.org/ontology#Zutat"/>
    </owl:Class>
    


    <!-- http://becks.org/ontology#Laktosefreie -->

    <owl:Class rdf:about="http://becks.org/ontology#Laktosefreie">
        <rdfs:subClassOf rdf:resource="http://becks.org/ontology#Zutat"/>
        <rdfs:comment>Enthält alle laktosefreien Zutaten, damit inferiert werden kann, ob eine Pizza laktosefrei ist.</rdfs:comment>
        <rdfs:label>Laktosefreie Zutaten</rdfs:label>
    </owl:Class>
    


    <!-- http://becks.org/ontology#Pizza -->

    <owl:Class rdf:about="http://becks.org/ontology#Pizza">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://becks.org/ontology#enthaeltZutat"/>
                <owl:someValuesFrom rdf:resource="http://becks.org/ontology#Sauce"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://becks.org/ontology#enthaeltZutat"/>
                <owl:someValuesFrom rdf:resource="http://becks.org/ontology#Teig"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Enthält konkrete Pizzen als Instanzen. Eine Pizza ist eine italienische Spezialität aus gebackenem, mit verschiedenen Zutaten belegtem Teig.</rdfs:comment>
        <rdfs:label>Pizza</rdfs:label>
    </owl:Class>
    


    <!-- http://becks.org/ontology#Pizza_FCC -->

    <owl:Class rdf:about="http://becks.org/ontology#Pizza_FCC">
        <rdfs:subClassOf rdf:resource="http://becks.org/ontology#Pizza_bei_Mekan"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://becks.org/ontology#enthaeltZutat"/>
                <owl:someValuesFrom rdf:resource="http://becks.org/ontology#Käse"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://becks.org/ontology#enthaeltZutat"/>
                <owl:hasValue rdf:resource="http://becks.org/ontology#Pepperoniwurst"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://becks.org/ontology#enthaeltZutat"/>
                <owl:hasValue rdf:resource="http://becks.org/ontology#Salami"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://becks.org/ontology#enthaeltZutat"/>
                <owl:hasValue rdf:resource="http://becks.org/ontology#Schinken"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Enthält alle Pizzen, die die Anforderungen an eine &quot;Pizza FCC&quot; erfüllen.</rdfs:comment>
        <rdfs:label>Pizza FCC</rdfs:label>
        <rdfs:label>Pizza Fußballclub Carl Zeiss Jena</rdfs:label>
    </owl:Class>
    


    <!-- http://becks.org/ontology#Pizza_Hawaii -->

    <owl:Class rdf:about="http://becks.org/ontology#Pizza_Hawaii">
        <rdfs:subClassOf rdf:resource="http://becks.org/ontology#Pizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://becks.org/ontology#enthaeltZutat"/>
                <owl:someValuesFrom rdf:resource="http://becks.org/ontology#Käse"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://becks.org/ontology#enthaeltZutat"/>
                <owl:hasValue rdf:resource="http://becks.org/ontology#Ananas"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://becks.org/ontology#enthaeltZutat"/>
                <owl:hasValue rdf:resource="http://becks.org/ontology#Schinken"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://becks.org/ontology#enthaeltZutat"/>
                <owl:hasValue rdf:resource="http://becks.org/ontology#Tomatensauce"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Enthält alle Pizzen, die die Anforderungen an eine &quot;Pizza Hawaii&quot; erfüllen.</rdfs:comment>
        <rdfs:label>Pizza Hawaii</rdfs:label>
    </owl:Class>
    


    <!-- http://becks.org/ontology#Pizza_bei_Mekan -->

    <owl:Class rdf:about="http://becks.org/ontology#Pizza_bei_Mekan">
        <rdfs:subClassOf rdf:resource="http://becks.org/ontology#Pizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://becks.org/ontology#enthaeltZutat"/>
                <owl:hasValue rdf:resource="http://becks.org/ontology#Käse"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Enthält Pizzen, die in der Pizzeria &quot;Mekan&quot; verkauft werden.</rdfs:comment>
        <rdfs:label>Pizza bei Mekan</rdfs:label>
    </owl:Class>
    


    <!-- http://becks.org/ontology#Pizzeria -->

    <owl:Class rdf:about="http://becks.org/ontology#Pizzeria">
        <rdfs:comment>Enthält konkrete Pizzerien als Instanzen. Eine Pizzeria ist eine Lokalität, welche Pizzen zum Verkauf anbietet.</rdfs:comment>
        <rdfs:label>Pizzeria</rdfs:label>
    </owl:Class>
    


    <!-- http://becks.org/ontology#Sauce -->

    <owl:Class rdf:about="http://becks.org/ontology#Sauce">
        <rdfs:subClassOf rdf:resource="http://becks.org/ontology#Zutat"/>
        <rdfs:comment>Enthält alle Saucen, die als Zutat Teil einer Pizza sein können.</rdfs:comment>
        <rdfs:label>Sauce</rdfs:label>
    </owl:Class>
    


    <!-- http://becks.org/ontology#Teig -->

    <owl:Class rdf:about="http://becks.org/ontology#Teig">
        <rdfs:subClassOf rdf:resource="http://becks.org/ontology#Zutat"/>
    </owl:Class>
    


    <!-- http://becks.org/ontology#Vegane -->

    <owl:Class rdf:about="http://becks.org/ontology#Vegane">
        <rdfs:subClassOf rdf:resource="http://becks.org/ontology#Laktosefreie"/>
        <rdfs:subClassOf rdf:resource="http://becks.org/ontology#Vegetarische"/>
        <rdfs:comment>Enthält alle veganen Zutaten, um eine Aussage darüber treffen zu können, ob eine konkrete Pizza vegan ist.</rdfs:comment>
        <rdfs:label>Vegane Zutaten</rdfs:label>
    </owl:Class>
    


    <!-- http://becks.org/ontology#Vegetarische -->

    <owl:Class rdf:about="http://becks.org/ontology#Vegetarische">
        <rdfs:subClassOf rdf:resource="http://becks.org/ontology#Zutat"/>
        <rdfs:comment>Enthält alle vegetarischen Zutaten, die Teil einer Pizza sein können.</rdfs:comment>
        <rdfs:label>Vegetarische Zutaten</rdfs:label>
    </owl:Class>
    


    <!-- http://becks.org/ontology#VegetarischePizza -->

    <owl:Class rdf:about="http://becks.org/ontology#VegetarischePizza">
        <rdfs:subClassOf rdf:resource="http://becks.org/ontology#Pizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://becks.org/ontology#enthaeltZutat"/>
                <owl:allValuesFrom rdf:resource="http://becks.org/ontology#Vegetarische"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Enthält vegetarische Pizzen, also Pizzen mit Zutaten, die allesamt vegetarisch sind.</rdfs:comment>
        <rdfs:label>Vegetarische Pizza</rdfs:label>
    </owl:Class>
    


    <!-- http://becks.org/ontology#VegetarischePizza_Hawaii -->

    <owl:Class rdf:about="http://becks.org/ontology#VegetarischePizza_Hawaii">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://becks.org/ontology#Pizza_Hawaii"/>
                    <rdf:Description rdf:about="http://becks.org/ontology#VegetarischePizza"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Enthält alle Pizzen Hawaii, die vegetarisch sind. Diese Schnittmenge ist leer.</rdfs:comment>
        <rdfs:label>Vegetarische Pizza Hawaii</rdfs:label>
    </owl:Class>
    


    <!-- http://becks.org/ontology#Zutat -->

    <owl:Class rdf:about="http://becks.org/ontology#Zutat">
        <rdfs:comment>Enthält alle möglichen Zutaten einer Pizza als Instanzen.</rdfs:comment>
        <rdfs:label>Zutat</rdfs:label>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://becks.org/ontology#Ananas -->

    <owl:NamedIndividual rdf:about="http://becks.org/ontology#Ananas">
        <rdf:type rdf:resource="http://becks.org/ontology#Glutenfreie"/>
        <rdf:type rdf:resource="http://becks.org/ontology#Vegane"/>
        <rdf:type rdf:resource="http://becks.org/ontology#Zutat"/>
        <rdfs:comment>Tropische Obstsorte. Die Ananaspflanze stammt aus der Familie der Bromeliengewächse.</rdfs:comment>
        <rdfs:label>Ananas</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://becks.org/ontology#Brokkoli -->

    <owl:NamedIndividual rdf:about="http://becks.org/ontology#Brokkoli">
        <rdf:type rdf:resource="http://becks.org/ontology#Vegane"/>
        <rdf:type rdf:resource="http://becks.org/ontology#Zutat"/>
        <rdfs:comment>Gemüsepflanze aus der Familie der Kreuzblütengewächse.</rdfs:comment>
        <rdfs:label>Brokkoli</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://becks.org/ontology#Käse -->

    <owl:NamedIndividual rdf:about="http://becks.org/ontology#Käse"/>
    


    <!-- http://becks.org/ontology#Maismehlteig -->

    <owl:NamedIndividual rdf:about="http://becks.org/ontology#Maismehlteig">
        <rdf:type rdf:resource="http://becks.org/ontology#Glutenfreie"/>
        <rdf:type rdf:resource="http://becks.org/ontology#Teig"/>
        <rdf:type rdf:resource="http://becks.org/ontology#Vegane"/>
        <rdf:type rdf:resource="http://becks.org/ontology#Zutat"/>
        <rdfs:comment>Teig aus Maismehl, Wasser und Salz.</rdfs:comment>
        <rdfs:label>Maismehlteig</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://becks.org/ontology#Mekan -->

    <owl:NamedIndividual rdf:about="http://becks.org/ontology#Mekan">
        <rdf:type rdf:resource="http://becks.org/ontology#Pizzeria"/>
        <adresse>Mekan, Johannisstraße 12, 07743 Jena, Deutschland (neben TTTTT)</adresse>
        <bewertungAufGoogle rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">4.0</bewertungAufGoogle>
        <rdfs:comment>Beliebtes Restaurant in der jenaer Innenstadt, welches unter anderem Pizza anbietet.</rdfs:comment>
        <rdfs:label>Mekan</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://becks.org/ontology#Mozzarella -->

    <owl:NamedIndividual rdf:about="http://becks.org/ontology#Mozzarella">
        <rdf:type rdf:resource="http://becks.org/ontology#Käse"/>
        <rdf:type rdf:resource="http://becks.org/ontology#Vegetarische"/>
        <rdfs:comment>Ein ursprünglich italienischer Filata-Käse, der aber auch außerhalb Italiens hergestellt wird.</rdfs:comment>
        <rdfs:label>Mozzarella</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://becks.org/ontology#Pepperoniwurst -->

    <owl:NamedIndividual rdf:about="http://becks.org/ontology#Pepperoniwurst">
        <rdf:type rdf:resource="http://becks.org/ontology#Zutat"/>
        <rdfs:comment>Scharf gewürzte Rohwurst aus Schweine- und/oder Rindfleisch.</rdfs:comment>
        <rdfs:label>Pepperoniwurst</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://becks.org/ontology#Pizza_FCC_bei_Mekan -->

    <owl:NamedIndividual rdf:about="http://becks.org/ontology#Pizza_FCC_bei_Mekan">
        <rdf:type rdf:resource="http://becks.org/ontology#Pizza"/>
        <rdf:type rdf:resource="http://becks.org/ontology#Pizza_FCC"/>
        <rdf:type rdf:resource="http://becks.org/ontology#Pizza_bei_Mekan"/>
        <enthaeltZutat rdf:resource="http://becks.org/ontology#Mozzarella"/>
        <enthaeltZutat rdf:resource="http://becks.org/ontology#Pepperoniwurst"/>
        <enthaeltZutat rdf:resource="http://becks.org/ontology#Salami"/>
        <enthaeltZutat rdf:resource="http://becks.org/ontology#Schinken"/>
        <enthaeltZutat rdf:resource="http://becks.org/ontology#Tomatensauce"/>
        <gehörtZuPizzeria rdf:resource="http://becks.org/ontology#Mekan"/>
        <groesse rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">28</groesse>
        <preis rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">7.9</preis>
        <rdfs:comment>Eine besondere Pizza-Kreation des Hauses, welche zu Ehren des glorreichen FC Carl Zeiss Jena erschaffen wurde.</rdfs:comment>
        <rdfs:label>Pizza FCC bei Mekan</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://becks.org/ontology#Pizza_Hawaii_bei_Mekan -->

    <owl:NamedIndividual rdf:about="http://becks.org/ontology#Pizza_Hawaii_bei_Mekan">
        <rdf:type rdf:resource="http://becks.org/ontology#Pizza"/>
        <rdf:type rdf:resource="http://becks.org/ontology#Pizza_Hawaii"/>
        <rdf:type rdf:resource="http://becks.org/ontology#Pizza_bei_Mekan"/>
        <enthaeltZutat rdf:resource="http://becks.org/ontology#Ananas"/>
        <enthaeltZutat rdf:resource="http://becks.org/ontology#Mozzarella"/>
        <enthaeltZutat rdf:resource="http://becks.org/ontology#Schinken"/>
        <enthaeltZutat rdf:resource="http://becks.org/ontology#Tomatensauce"/>
        <gehörtZuPizzeria rdf:resource="http://becks.org/ontology#Mekan"/>
        <groesse rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">28</groesse>
        <preis rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">7.9</preis>
        <rdfs:comment>Die Pizza Hawaii, die bei Mekan angeboten wird.</rdfs:comment>
        <rdfs:label>Pizza Hawaii bei Mekan</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://becks.org/ontology#Reismehlteig -->

    <owl:NamedIndividual rdf:about="http://becks.org/ontology#Reismehlteig">
        <rdf:type rdf:resource="http://becks.org/ontology#Glutenfreie"/>
        <rdf:type rdf:resource="http://becks.org/ontology#Teig"/>
        <rdf:type rdf:resource="http://becks.org/ontology#Vegane"/>
        <rdf:type rdf:resource="http://becks.org/ontology#Zutat"/>
        <rdfs:comment>Teig aus Reismehl, Wasser und Salz.</rdfs:comment>
        <rdfs:label>Reismehlteig</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://becks.org/ontology#Salami -->

    <owl:NamedIndividual rdf:about="http://becks.org/ontology#Salami">
        <rdf:type rdf:resource="http://becks.org/ontology#Zutat"/>
        <rdfs:comment>Eine Rohwurstsorte aus Schweinefleisch und anderen Fleischsorten, die in Italien erfunden wurde.</rdfs:comment>
        <rdfs:label>Salami</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://becks.org/ontology#Schinken -->

    <owl:NamedIndividual rdf:about="http://becks.org/ontology#Schinken">
        <rdf:type rdf:resource="http://becks.org/ontology#Zutat"/>
        <rdfs:comment>(Meist) Schweinefleisch aus der Keule, das durch Pökeln konserviert wurde.</rdfs:comment>
        <rdfs:label>Schinken</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://becks.org/ontology#Teig -->

    <owl:NamedIndividual rdf:about="http://becks.org/ontology#Teig">
        <rdf:type rdf:resource="http://becks.org/ontology#Vegane"/>
        <rdf:type rdf:resource="http://becks.org/ontology#Zutat"/>
    </owl:NamedIndividual>
    


    <!-- http://becks.org/ontology#Tomatensauce -->

    <owl:NamedIndividual rdf:about="http://becks.org/ontology#Tomatensauce">
        <rdf:type rdf:resource="http://becks.org/ontology#Laktosefreie"/>
        <rdf:type rdf:resource="http://becks.org/ontology#Sauce"/>
        <rdf:type rdf:resource="http://becks.org/ontology#Vegane"/>
        <rdfs:comment>Sauce aus Tomaten und optional weiteren Zutaten wie Knoblauch, Zwiebeln und Olivenöl, sowie verschiedenen Gewürzen und Kräutern.</rdfs:comment>
        <rdfs:label>Tomatensauce</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://becks.org/ontology#Weizenmehlteig -->

    <owl:NamedIndividual rdf:about="http://becks.org/ontology#Weizenmehlteig">
        <rdf:type rdf:resource="http://becks.org/ontology#Laktosefreie"/>
        <rdf:type rdf:resource="http://becks.org/ontology#Teig"/>
        <rdf:type rdf:resource="http://becks.org/ontology#Vegane"/>
        <rdf:type rdf:resource="http://becks.org/ontology#Zutat"/>
        <rdfs:comment>Teig aus Weizenmehl, Wasser und Salz. Optional können Öl und Hefe enthalten sein.</rdfs:comment>
        <rdfs:label>Weizenmehlteig</rdfs:label>
    </owl:NamedIndividual>
    <rdf:Description>
        <owl:qualifiedCardinality>1</owl:qualifiedCardinality>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotations
    //
    ///////////////////////////////////////////////////////////////////////////////////////
